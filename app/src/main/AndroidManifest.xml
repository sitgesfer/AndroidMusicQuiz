<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicquiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NotesActivity" android:label="Musical Notes" />
        <activity android:name=".InstrumentsActivity" android:label="Musical Instruments" />
        <activity android:name=".VoicesActivity" android:label="Choir Voices" />
        <activity android:name=".ComposersActivity" android:label="Classical Composers"/>
    </application>

</manifest>